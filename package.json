{
  "name": "ac",
  "version": "0.0.1",
  "repository": {
    "url": "git@github.com:np-at/a11y-carousel.git"
  },
  "description": "an attempt to create a more accessible web carousel component using stenciljs",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ac/ac.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve --no-open --debug",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint src/*/**/*{.ts,.tsx}"
  },
  "dependencies": {
    "@stencil/core": "^2.5.2"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.4.1",
    "@types/jest": "^26.0.23",
    "@types/puppeteer": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^9.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.2.4"
  },
  "license": "MIT"
}
